  },
  "externals": {},
  "localizedResources": {
    "CircoStudiaWebPartStrings": "lib/webparts/circoStudia/loc/{locale}.js",
    "OfertaMateriasIngInfWebPartStrings": "lib/webparts/ofertaMateriasIngInf/loc/{locale}.js",
    "PlanDeEstudioIngInfWebPartStrings": "lib/webparts/planDeEstudioIngInf/loc/{locale}.js",
    "HistorialAcademicoWebPartStrings": "lib/webparts/historialAcademico/loc/{locale}.js",
    "AppCircoStudiaWebPartStrings": "lib/webparts/appCircoStudia/loc/{locale}.js"
  }
