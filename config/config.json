{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "circo-studia-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/circoStudia/CircoStudiaWebPart.js",
          "manifest": "./src/webparts/circoStudia/CircoStudiaWebPart.manifest.json"
        }
      ]
    },
    "oferta-materias-ing-inf-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ofertaMateriasIngInf/OfertaMateriasIngInfWebPart.js",
          "manifest": "./src/webparts/ofertaMateriasIngInf/OfertaMateriasIngInfWebPart.manifest.json"
        }
      ]
    },
    "plan-de-estudio-ing-inf-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/planDeEstudioIngInf/PlanDeEstudioIngInfWebPart.js",
